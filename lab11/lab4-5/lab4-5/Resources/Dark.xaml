<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="Wind" TargetType="Window">
        <Setter Property="Background" Value="#344756" />
    </Style>
    <Style x:Key="Grid" TargetType="Menu">
        <Setter Property="Background" Value="#595B82" />
    </Style>
    <Style x:Key="List" TargetType="Grid">
        <Setter Property="Background" Value="#595B82" />
    </Style>
    <Style x:Key="ListBox" TargetType="ListBox">
        <Setter Property="Background" Value="#595B82" />
    </Style>
</ResourceDictionary>